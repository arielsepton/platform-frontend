    "eslint-plugin-react-refresh": "^0.4.6",
    "happy-dom": "^14.10.1",
    "msw": "^2.3.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.2.2",
    "vite": "^5.2.11",
    "vite-plugin-svgr": "^4.2.0",
    "vitest": "^1.6.0"
  }
